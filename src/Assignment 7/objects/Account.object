<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>VR_AccountNumberMustBeEntered</fullName>
        <active>true</active>
        <description>You cannot create any account without entering Account Number</description>
        <errorConditionFormula>ISBLANK( AccountNumber )</errorConditionFormula>
        <errorDisplayField>AccountNumber</errorDisplayField>
        <errorMessage>Please enter Account Number.</errorMessage>
    </validationRules>
</CustomObject>
